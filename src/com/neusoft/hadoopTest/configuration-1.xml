<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <property>
        <name>color</name>
        <value>yellow</value>
        <descrition>Color</descrition>
    </property>
    <property>
        <name>size</name>
        <value>10</value>
        <descrition>Size</descrition>
    </property>
    <property>
        <name>wight</name>
        <value>heavy</value>
